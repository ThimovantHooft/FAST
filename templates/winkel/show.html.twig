{% extends 'base.html.twig' %}

{% block title %}Winkel{% endblock %}

{% block body %}
    <h1>Winkel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ winkel.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ winkel.name }}</td>
            </tr>
            <tr>
                <th>Besch</th>
                <td>{{ winkel.besch }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ winkel.email }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ winkel.adress }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ winkel.tel }}</td>
            </tr>
        </tbody>
    </table>

    <button type="button"  class="btn btn-info"><a  style="color: white"href="{{ path('winkel_index') }}">back to list</a></button>


{% endblock %}
